<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <Button
        android:id="@+id/business_button_getsvrlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="75dp"
        android:onClick="getsvrlist"
        android:text="@string/business_main_botton_getsvrlist" />

    <Button
        android:id="@+id/business_button_getproducts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/business_button_getsvrlist"
        android:layout_alignLeft="@+id/business_button_getsvrlist"
        android:layout_alignStart="@+id/business_button_getsvrlist"
        android:layout_alignRight="@+id/business_button_getsvrlist"
        android:layout_below="@+id/business_button_getsvrlist"
        android:layout_marginTop="33dp"
        android:onClick="getproducts"
        android:text="@string/business_main_botton_getproducts" />

</RelativeLayout>
